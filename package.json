{
    "name": "ide-erlang",
    "main": "./lib/main",
    "version": "0.0.0",
    "description": "Erlang LSP client for Atom using atom-languageclient",
    "keywords": [
        "erlang",
        "atom-ide",
        "autocomplete",
        "languageserver",
        "language-server-protocol",
        "language-client"
    ],
    "activationCommands": {
        "atom-workspace": "ide-erlang:toggle"
    },
    "repository": "https://github.com/juliusan/ide-erlang",
    "license": "MIT",
    "engines": {
        "atom": ">=1.0.0 <2.0.0"
    },
    "dependencies": {
        "atom-languageclient": "0.9.9",
        "decompress": "^4.2.0"
    },
    "activationHooks": [
        "core:loaded-shell-environment"
    ],
    "consumedServices": {
        "linter-indie": {
          "versions": {
            "2.0.0": "consumeLinterV2"
          }
        },
        "datatip": {
          "versions": {
            "0.1.0": "consumeDatatip"
          }
        },
        "status-bar": {
            "versions": {
                "^1.0.0": "consumeStatusBar"
            }
        }
    },
    "providedServices": {
        "autocomplete.provider": {
          "versions": {
            "2.0.0": "provideAutocomplete"
          }
        },
        "code-format.range": {
          "versions": {
            "0.1.0": "provideCodeFormat"
          }
        },
        "code-highlight": {
          "versions": {
            "0.1.0": "provideCodeHighlight"
          }
        },
        "definitions": {
          "versions": {
            "0.1.0": "provideDefinitions"
          }
        },
        "find-references": {
          "versions": {
            "0.1.0": "provideFindReferences"
          }
        },
        "outline-view": {
          "versions": {
            "0.1.0": "provideOutlines"
          }
        }
    }
}
